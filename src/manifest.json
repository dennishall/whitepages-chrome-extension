{
  "name" : "Whitepages Helper",
  "version" : "1.0",
  "manifest_version": 2,
  "description" : "Hides a couple buttons, makes it easier to copy and paste addresses.",
  "permissions" : [
    "clipboardRead",
    "clipboardWrite"
  ],
  "content_scripts" : [
    {
      "matches" : [
        "http://www.whitepages.com/*"
      ],
      "js" : ["contentscript.js"],
      "css" : ["contentstyle.css"],
      "all_frames" : false
    }
  ]
}
